type Query {
    UserById(id: ID): User
}

type User {
    id: ID
    name: String
    username: String
    address: Address
    phone: String
    website: String
    company: Company
    posts: [Post]
}
type Address {
    street: String
    suite: String
    city: String
    zipcode: String
}

type Company {
    name: String
    catchPhrase: String
    bs: String
}


type Post {
    id: ID
    title: String
    userId: String
    body: String
}

