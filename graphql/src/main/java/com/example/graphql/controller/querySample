# Coppy to http://localhost:8080/graphiql?path=/graphql to see how it work !
query graphql {
  UserById(id: "1") {
    id
    name
    username
    address {
      street
      suite
      city
      zipcode
    }
    phone
    website
    username
    company {
      name
      catchPhrase
      bs
    }
    posts {
      id
      title
      body
    }
  }
}